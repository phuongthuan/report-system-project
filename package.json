{
  "name": "react-template",
  "version": "1.0.0",
  "description": "react boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "./node_modules/.bin/webpack-dev-server --config ./internals/webpack/webpack.config.js --mode=development",
    "build": "webpack --config ./internals/webpack/webpack.config.js --mode=production",
    "seed": "node server/generateData.js > server/db.json",
    "serve": "node server/server.js"
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "nguyen.van.thang@framgia.com",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-11",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^2.0.0",
    "antd": "^3.7.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "chartjs-plugin-datalabels": "^0.3.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "emoji-mart": "^2.6.1",
    "emoji-mart-picker": "^0.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "faker": "^4.1.0",
    "formik": "^1.0.2",
    "history": "^4.7.2",
    "immutability-helper": "^2.7.1",
    "json-server": "^0.14.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0-beta.7",
    "react-transition-group": "^2.4.0",
    "reactstrap": "^6.2.0",
    "recharts": "^1.0.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "regenerator-runtime": "^0.12.0",
    "style-loader": "^0.21.0",
    "styled-components": "^3.3.3",
    "uuid": "^3.3.2",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.4",
    "yup": "^0.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-recharts": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.2.0",
    "webpack-cli": "3.0.8"
  },
  "babel": {
    "plugins": [
      "recharts",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ],
      [
        "react-intl", {
          "messagesDir": "./build/messages",
          "enforceDescriptions": false
        }
      ]
    ],
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/containers/**/*.{js,jsx}",
      "app/components/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/jest-config.js"
  }
}
